---
import Layout from "../layouts/Layout.astro";
import CardCarousel from '../components/CardCarousel.astro';
import marketData from "../data/markets.json";

---
<Layout title="Display of static data in card form:" >
{
    marketData.map((marketData) => {
    return (
        <CardCarousel
        title={marketData.title}
        description={marketData.description}
        betOptions={marketData.betOptions}
        betPercentage={marketData.betPercentage}
        imageUrl={marketData.imageUrl}
        />
    );
    })
}
</Layout>